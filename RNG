import random

#sets dicts to keep track of people

dct_name = {"Svitan Daniel": random.randint(1,10000), "Tulek Jakub": random.randint(1,10000)}
people = ["Svitan Daniel", "Tulek Jakub"]

for nme in dct_name:
    if nme in people:
        dct_name[nme] = dct_name.get(nme) * 0.75
        print(dct_name[nme])

wwchd = 0

for nme in dct_name:
    if dct_name[nme] > wwchd:
        wwchd = int(dct_name[nme])
    elif dct_name[nme] <= wwchd:
        pass

str(wwchd)


winner = dct_name.get(wwchd)
print(winner)



