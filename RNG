dct_name = {"Dano": 3746}
dct_index = {dct_name["Dano"]: "Dano"}


wwchd = 0

for name in dct_name:
    if int(dct_name[name]) > wwchd:
        wwchd = int(dct_name[name])
    elif int(dct_name[name]) <= wwchd:
        pass



def GetNameOfNum(wwchd):
    winner = dct_index[wwchd]
    print(winner)

GetNameOfNum(wwchd)
